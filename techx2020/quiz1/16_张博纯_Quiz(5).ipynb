    "def binary(n):\n",
    "    re=list()\n",
    "    n,m=divmod(n,2)\n",
    "    re.append(str(m))\n",
    "    while n!=0:\n",
    "        n,m=divmod(n,2)\n",
    "        re.insert(0,str(m))\n",
    "    print(''.join(re))"
   ]
